#include <stdio.h>

char* match(char* s, char ch1, char ch2)
{
    char* ch1_addr = NULL;
    for (char* p = s; *p != '\0'; p++)
    {
        if (*p == ch1)
        {
            ch1_addr = p;
            break;
        }
    }

    if (ch1_addr == NULL)
    {
        printf("\n");
        return NULL;
    }
    for (char* p = ch1_addr; *p != '\0'; p++)
    {
        putchar(*p);
        if (*p == ch2)
            break;
    }
	printf("\n");
    return ch1_addr;
}

#define MAXS 10

int main()
{
    char str[MAXS], ch_start, ch_end, * p;

    scanf("%s\n", str);
    scanf("%c %c", &ch_start, &ch_end);
    p = match(str, ch_start, ch_end);
    printf("%s\n", p);

    return 0;
}
