struct stud_node* daletelist(struct stud_node* head, int min_score)
{
    struct stud_node* curr = head, * temp;
    while (head != NULL && head->score < min_score)
    {
        temp = head;
        head = head->next;
		free(temp);
    }
    curr = head;
    while (curr != NULL && curr->next != NULL)
    {
        if (curr->next->score < min_score)
        {
            temp = curr->next;
            curr->next = curr->next->next;
            free(temp);
        }
        else
            curr = curr->next;
    }
    return head;
}
